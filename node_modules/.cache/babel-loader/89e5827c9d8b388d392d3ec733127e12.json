,MADA,EACA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,CAAA,EAAA,WAAA,KAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,0BAJA,wCAIA;AACA,WAAA,mBAAA,GAAA,CAAA,KAAA,mBAAA;AACA,KANA;AAOA,IAAA,aAPA,2BAOA;AACA,WAAA,mBAAA,GAAA,KAAA;AACA,KATA;AAUA,IAAA,aAVA,2BAUA;AACA,WAAA,QAAA,CAAA,cAAA,CAAA,CAAA,KAAA,QAAA,CAAA,WAAA;AACA,KAZA;AAaA,IAAA,WAbA,yBAaA;AACA,WAAA,QAAA,CAAA,cAAA,CAAA,KAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,wBAgBA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,SAnBA,uBAmBA;AACA,UAAA,GAAA,GAAA,IAAA,CADA,CAEA;;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA,KAvBA;AAwBA,IAAA,aAxBA,2BAwBA;AACA,UAAA,GAAA,GAAA,IAAA;AACA,MAAA,cAAA,CAAA,MAAA;AACA,MAAA,GAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AAEA;AA7BA;AAtBA,CAAA","sourcesContent":["<template> \n  <nav class=\"navbar navbar-expand-lg navbar-absolute\"\n       :class=\"{'bg-white': showMenu, 'navbar-transparent': !showMenu}\">\n\n\n    <div class=\"container-fluid\">\n      <header class=\"map-header\">\n      <h1 class=\"brand\">\n        <span class=\"brand-title\">Redex</span><span class=\"brand-title-point\">.</span>\n      </h1>\n      <p class=\"brand-tagline\">Always on Time</p>\n   \n    </header>\n\n      <div class=\"navbar-wrapper\">\n        <div class=\"navbar-toggle d-inline\" :class=\"{toggled: $sidebar.showSidebar}\">\n     \n          <button type=\"button\"\n                  class=\"navbar-toggler\"\n                  aria-label=\"Navbar toggle button\"\n                  @click=\"toggleSidebar\">\n                  \n            <span class=\"navbar-toggler-bar bar1\"></span>\n            <span class=\"navbar-toggler-bar bar2\"></span>\n            <span class=\"navbar-toggler-bar bar3\"></span>\n          </button>\n        </div>\n        \n        <p class=\"navbar-brand\">{{routeName}}</p>\n      </div>\n\n\n      <button class=\"navbar-toggler\" type=\"button\"\n              @click=\"toggleMenu\"\n              data-toggle=\"collapse\"\n              data-target=\"#navigation\"\n              aria-controls=\"navigation-index\"\n              aria-label=\"Toggle navigation\">\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n      </button>\n\n      <collapse-transition>\n        <div class=\"collapse navbar-collapse show\" v-show=\"showMenu\">\n          <ul class=\"navbar-nav\" :class=\"$rtl.isRTL ? 'mr-auto' : 'ml-auto'\">\n          \n            <base-dropdown tag=\"li\"\n                           :menu-on-right=\"!$rtl.isRTL\"\n                           title-tag=\"a\"\n                           class=\"nav-item\"\n                           menu-classes=\"dropdown-navbar\">\n              <a slot=\"title\" href=\"#\" class=\"dropdown-toggle nav-link\" data-toggle=\"dropdown\" aria-expanded=\"true\">\n                <div class=\"photo\">\n                  <img src=\"img/anime3.png\">\n                </div>\n                <b class=\"caret d-none d-lg-block d-xl-block\"></b>\n                <p class=\"d-lg-none\">\n                  Salir\n                </p>\n              </a>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\" @click=\"goProfile()\">Perfil</a>\n              </li>\n\n              <div class=\"dropdown-divider\"></div>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\" @click=\"logoutProfile()\">Salir</a>\n              </li>\n            </base-dropdown>\n          </ul>\n        </div>\n      </collapse-transition>\n    </div>\n  </nav>\n</template>\n<script>\n  import { CollapseTransition } from 'vue2-transitions';\n  import Modal from '@/components/Modal';\n\n  import Authentication from '@/store/authentication.js';\n\n  export default {\n    components: {\n      CollapseTransition,\n      Modal\n    },\n    computed: {\n      routeName() {\n        const { name } = this.$route;\n        return this.capitalizeFirstLetter(name);\n      },\n      isRTL() {\n        return this.$rtl.isRTL;\n      }\n    },\n    data() {\n      return {\n        activeNotifications: false,\n        showMenu: false,\n        searchModalVisible: false,\n        searchQuery: ''\n      };\n    },\n    methods: {\n      capitalizeFirstLetter(string) {\n        return string.charAt(0).toUpperCase() + string.slice(1);\n      },\n      toggleNotificationDropDown() {\n        this.activeNotifications = !this.activeNotifications;\n      },\n      closeDropDown() {\n        this.activeNotifications = false;\n      },\n      toggleSidebar() {\n        this.$sidebar.displaySidebar(!this.$sidebar.showSidebar);\n      },\n      hideSidebar() {\n        this.$sidebar.displaySidebar(false);\n      },\n      toggleMenu() {\n        this.showMenu = !this.showMenu;\n      },\n      goProfile(){\n        let vue = this;\n        //Authentication.logout();\n        vue.$router.push('/profile');\n      },\n      logoutProfile(){\n        let vue = this;\n        Authentication.logout();\n        vue.$router.push('/login');\n\n      }\n    }\n  };\n</script>\n<style>\n.brand-title {\n  font-size: 1em; /* Ajusta el tamaño como desees */\n  color: #fff; /* Color para 'Redex' */\n  font-weight: bold;\n}\n\n.brand-title-point {\n  font-size: 1em; /* Asegúrate de que sea del mismo tamaño que .brand-title para alineación adecuada */\n  color: #00B074; /* Color para el punto */\n  font-weight: bold;\n}\n\n.brand-tagline {\n  font-size: 1em; /* Ajusta el tamaño del tagline */\n  color: #363636; /* Color para 'Always on Time' */\n  font-weight: bold;\n\n  display: flex;\n  align-items: baseline; /* Alinea correctamente el texto y el punto */\n  gap: 0.2em; /* Ajusta el espacio entre 'Redex' y el punto */\n  margin-top: -15px;\n}\n\n/* Si deseas que 'Redex.' esté en una sola línea, puedes usar display: flex en el contenedor */\n.brand {\n  display: flex;\n  align-items: baseline; /* Alinea correctamente el texto y el punto */\n  gap: 0.2em; /* Ajusta el espacio entre 'Redex' y el punto */\n  \n  margin-bottom: 15px; /* Ajusta esto para aumentar el espacio entre el título y el subtítulo */\n\n}\n.navbar-brand {\n  margin-right: 20px; \n  font-weight: bold;\n}\n.container-fluid {\n  display: flex;\n  align-items: center; /* Asegura que todo esté alineado verticalmente */\n  justify-content: space-between; /* Distribuye el espacio entre los elementos */\n}\n.navbar-header, .navbar-profile {\n  display: flex;\n  align-items: center;\n}\n.map-header, .navbar-profile {\n  flex: 1; /* Ambos toman igual cantidad de espacio */\n  justify-content: center; /* Centra los elementos dentro de cada contenedor */\n  text-align: center; /* Centra el texto dentro de los elementos */\n}\n.navbar-header {\n  display: flex;\n  align-items: center; /* Asegura que todo esté alineado verticalmente */\n  margin-top: 200px;\n}\n\n.map-header {\n  margin-right: 20px; /* Ajusta el margen según sea necesario */\n}\n.dropdown-toggle.nav-link {\n  display: flex;\n  align-items: center; /* Asegura que la imagen y el texto estén alineados verticalmente */\n  margin-bottom: 20%;\n}\n\n.photo img {\n  width: 40px; /* Ajusta el tamaño de la imagen según sea necesario */\n  height: 40px;\n  border-radius: 50%; /* Hace la imagen circular */\n  margin-right: 10px; /* Añade margen a la derecha de la imagen */\n\n}\n</style>"],"sourceRoot":"src/layout/dashboard"}]}